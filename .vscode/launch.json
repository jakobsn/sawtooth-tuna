{
    "version": "0.2.0",
    "configurations": [
      {
        "type": "pwa-node",
        "request": "attach",
        "name": "Debug: tunachain-backend",
        "remoteRoot": "/project/sawtooth-tuna/backend",
        "localRoot": "${workspaceFolder}/backend",
        "protocol": "inspector",
        "port": 9229,
        "restart": true,
        "address": "0.0.0.0",
        "skipFiles": [
          "${workspaceFolder}/backend/node_modules/**/*.js",
          "<node_internals>/**/*.js"
        ],
        "resolveSourceMapLocations": [
          "${workspaceFolder}/backend/**",
          "!**/node_modules/**"
        ]
      }
    ]
   }